   => (pre(identity(d),y) = y <=> fst(<<pre(identity(d),y),y>>) = snd(<<pre(identity(d),y),y>>))
   BECAUSE \A(x <- pre(identity(d),y), y <- fst(<<pre(identity(d),y),y>>), a <- y, b <- snd(<<pre(identity(d),y),y>>));
