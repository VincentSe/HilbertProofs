<<x,z>> \in identity(d) <=> <<x,z>> \in d \X d /\ fst(<<x,z>>) = snd(<<x,z>>)
   BECAUSE \A(d <- d, x <- <<x,z>>);
<<x,z>> \in identity(d) => <<x,z>> \in d \X d /\ fst(<<x,z>>) = snd(<<x,z>>)
   BECAUSE EquivToImpl;
<<x,z>> \in d \X d /\ fst(<<x,z>>) = snd(<<x,z>>)
 => fst(<<x,z>>) = snd(<<x,z>>)   BECAUSE S22;
