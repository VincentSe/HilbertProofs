<<pre(identity(d),y),y>> \in identity(d) => fst(<<pre(identity(d),y),y>>) = snd(<<pre(identity(d),y),y>>)   BECAUSE TI;
y \in d => fst(<<pre(identity(d),y),y>>) = snd(<<pre(identity(d),y),y>>)   BECAUSE TI;
